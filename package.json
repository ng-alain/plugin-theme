{
  "name": "ng-alain-plugin-theme",
  "version": "13.0.3",
  "description": "NG-ALAIN theme plugin",
  "keywords": [
    "delon",
    "ng-alain",
    "ng-alain-plugin"
  ],
  "author": "cipchk <cipchk@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ng-alain/plugin-theme/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng-alain/plugin-theme.git"
  },
  "homepage": "https://ng-alain.com",
  "bin": {
    "ng-alain-plugin-theme": "lib/index.js"
  },
  "scripts": {
    "tsc": "tsc",
    "watch": "tsc --w",
    "lint": "tslint -p tsconfig.json -c tslint.json 'lib/**/*.ts'",
    "build": "bash ./build.sh",
    "build:test": "bash ./build.sh -t",
    "test": "TS_NODE_PROJECT=./test/tsconfig.json npm run mocha --recursive ./test/**/*.spec.ts",
    "mocha": "mocha -r ts-node/register",
    "release": "npm run build && cd dist && npm publish --access public"
  },
  "dependencies": {
    "meow": "^7.1.0",
    "postcss": "^7.0.0",
    "hash.js": "^1.0.0",
    "strip-css-comments": "^4.1.0",
    "less": "^4.1.2",
    "less-plugin-clean-css": "^1.5.1",
    "less-plugin-npm-import": "^2.1.0",
    "less-vars-to-js": "^1.3.0",
    "jsonc-parser": "^3.0.0"
  },
  "devDependencies": {
    "@types/meow": "^6.0.0",
    "@types/node": "^14.0.27",
    "typescript": "^4.3.2",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3 ",
    "tslint-config-prettier": "^1.18.0",
    "mocha": "^9.1.3",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "@types/chai": "^4.3.0",
    "@types/less": "^3.0.3"
  }
}
